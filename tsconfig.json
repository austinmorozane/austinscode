{

 "compilerOptions": {
   "jsx": "react-jsx+ react-dom",
    "target": "esnext",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "moduleResolution": "esnext",
    "resolveJsonModule": true
  },
  "optimization": {
"minimizer":[
	{
		"name":"terser",
		"options":{
			"parallel": true
		}
	}
]
  },

  "include": [
    "src"
  ]
}
